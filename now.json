{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@now/next" },
    { "src": "static/sitemap.xml", "use": "@now/static" }
  ],
  "routes": [
    { "src": "/events", "status": 301, "headers": { "Location": "/page/events" } },
    { "src": "/about", "status": 301, "headers": { "Location": "/page/about" } },
    { "src": "/internship", "status": 301, "headers": { "Location": "/page/internship" } },
    { "src": "/nextsteps", "status": 301, "headers": { "Location": "/page/nextsteps" } },
    { "src": "/smallgroups", "status": 301, "headers": { "Location": "/page/smallgroups" } },
    { "src": "/app", "status": 301, "headers": { "Location": "/page/app" } },
    { "src": "/sitemap.xml", "dest": "/static/sitemap.xml" }
  ]
}
